{
  "name": "schaufel-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postversion": "./scripts/postversion.sh $npm_package_version"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  }
}
